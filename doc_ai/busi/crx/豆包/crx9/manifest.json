{
    "manifest_version": 3,
    "name": "请求拦截替换插件",
    "version": "1.0",
    "description": "拦截并替换请求地址",
    "permissions": [
        "webRequest",
        "storage",
        "http://*/*",
        "https://*/*",
        "<all_urls>",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}    